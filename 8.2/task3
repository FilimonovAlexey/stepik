#include <stdio.h>

int main() {
  int n = 0;
  scanf ("%d", &n);
  int matrix[n][n];
  for (int i=0; i<n; i++) {
      if (i%2==0) {
          for (int j=0; j<n; j++){
              printf ("%d ", j+1);    
          }
      } else {
          for (int j=n-1; j>=0; j--){
              printf ("%d ", j+1);    
          }
      }
      printf ("\n");
  }

  return 0;
}