#include <stdio.h>

int main() {
    char str[100];
    while (1) {
        int count = scanf ("%s", &str);
        if (count <=0) break;
        int length = strlen(str);        
        for (int j=strlen(str)-1; j>=0; j--) {
            printf ("%c", str[j]);
        }
        
        printf (" ");
    }
    return 0;
}