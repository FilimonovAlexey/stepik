#include <stdio.h>

void square(int x, char c){
    for (int i=0; i<x; i++) {
        for (int j=0; j<x; j++) {
            printf ("%c", c);
        }
        printf ("\n");
    }
    return 0;
}

int main(){ 
    int n = 0;
    int symb = 'a';
    
    scanf("%d %c",&n, &symb);
    
    square(n, symb);
    
    return 0;
}