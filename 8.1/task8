#include <stdio.h>

int main() {
  int n = 0;
  scanf ("%d", &n);
  int arr[n/2];
    
  for (int i=0; i<n; i++) {
      if (i<n/2) {
          scanf ("%d", &arr[i]);
      }
      else {
          int temp = 0;
          scanf ("%d", &temp);
          printf ("%d ", temp);
      }
  }
  for (int i=0; i<n/2; i++) {
      printf ("%d ", arr[i]);
  }
  return 0;
}